# Required Configuration
# ===================

# Jira Configuration
JIRA_URL=https://your-domain.atlassian.net
JIRA_EMAIL=your-email@example.com
JIRA_API_TOKEN=your-jira-api-token

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key


# Optional Configuration
# ====================

# Data Storage
DATA_DIR=data  # Base directory for all data storage (cache, embeddings, results)

# Logging
LOG_LEVEL=INFO  # One of: DEBUG, INFO, WARNING, ERROR, CRITICAL

# Model Configuration
MODEL_TYPE=linear  # 'linear', 'random_forest' or 'neural'
EMBEDDING_MODEL=ada  # Default embedding model (ada = text-embedding-ada-002)

# Default Model Parameters
DEFAULT_TEST_SIZE=0.2        # Train/test split ratio
DEFAULT_CV_SPLITS=5          # Number of cross-validation folds
DEFAULT_RANDOM_SEED=42       # Random seed for reproducibility
DEFAULT_N_ESTIMATORS=100     # Number of trees in random forest
DEFAULT_EPOCHS=100           # Training epochs for neural network
DEFAULT_BATCH_SIZE=32        # Batch size for neural network
DEFAULT_LEARNING_RATE=0.001  # Learning rate for neural network
DEFAULT_MAX_DEPTH=-1         # Maximum tree depth (-1 = unlimited)
DEFAULT_MIN_SAMPLES_SPLIT=2  # Minimum number of samples required to split an internal node
DEFAULT_MIN_SAMPLES_LEAF=1   # Minimum number of samples required to be leaf node
DEFAULT_MAX_FEATURES='sqrt'  # Maximum number of features for random forest
DEFAULT_BOOTSTRAP=True       # Bootstrap samples when building trees
